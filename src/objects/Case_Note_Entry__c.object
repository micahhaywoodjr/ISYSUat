    <validationRules>
        <fullName>Case_Note_00</fullName>
        <active>false</active>
        <description>Throws an error if the case note is a 00 - needs to be changed to a valid case note</description>
        <errorConditionFormula>Billing_Code__r.Name  = &quot;00-To Be Updated by CM&quot;</errorConditionFormula>
        <errorMessage>Please update all Activity Codes, 00-Case Manager to update must be changed to a valid Activity Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Restriction</fullName>
        <active>true</active>
        <description>Activity Date must be inside From and Through Date</description>
        <errorConditionFormula>OR(
(Activity_Date__c &gt;  Report__r.Service_Date_Thru__c),
(Activity_Date__c &lt;  Report__r.Service_Date_From__c)
)</errorConditionFormula>
        <errorDisplayField>Activity_Date__c</errorDisplayField>
        <errorMessage>Activity date must be within the Service Date Thru and Service Date From on the related Case Report.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DisallowNONProfNULLValues</fullName>
        <active>true</active>
        <description>Nurse Prof or non-prof can&apos;t be null, based off the billing code applied.</description>
        <errorConditionFormula>(LEFT(Billing_Code__r.Name, 1) = &quot;T&quot; ||
LEFT(Billing_Code__r.Name, 1) = &quot;W&quot;) &amp;&amp;
ISBLANK(Travel_Wait_Hours_Nurse__c)</errorConditionFormula>
        <errorDisplayField>Travel_Wait_Hours_Nurse__c</errorDisplayField>
        <errorMessage>Non-Professional Units cannot be NULL, please enter a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DisallowProfNULLValues</fullName>
        <active>true</active>
        <description>Nurse Prof or non-prof can&apos;t be null, based off the billing code applied.</description>
        <errorConditionFormula>(LEFT(Billing_Code__r.Name, 1) &lt;&gt; &quot;T&quot; &amp;&amp;
LEFT(Billing_Code__r.Name, 1) &lt;&gt; &quot;W&quot;) &amp;&amp;
ISBLANK(Professional_Hours_Nurse__c)</errorConditionFormula>
        <errorDisplayField>Professional_Hours_Nurse__c</errorDisplayField>
        <errorMessage>Professional Units cannot be NULL, please enter a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exceed_Allowable_Limits_Professional</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Report__r.StageName,&quot;In Process&quot;),
(Professional_Hours__c &gt;  Billing_Code__r.Max__c),
(Professional_Hours__c &lt;  Billing_Code__r.Min__c)
)</errorConditionFormula>
        <errorDisplayField>Professional_Hours__c</errorDisplayField>
        <errorMessage>Professional units exceed the allowable guideline.  Please adjust.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MaxAllowableLimitsProfNurse</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Report__r.StageName,&quot;In Process&quot;),
Professional_Hours_Nurse__c &gt; Billing_Code__r.Max__c,
(LEFT( Billing_Code__r.Name, 1) &lt;&gt; &quot;T&quot;),
(LEFT(Billing_Code__r.Name, 1) &lt;&gt; &quot;W&quot;))</errorConditionFormula>
        <errorDisplayField>Professional_Hours_Nurse__c</errorDisplayField>
        <errorMessage>Professional units exceed the Maximum allowable guideline. Please adjust.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MaxAllowableLimitsTravelWait</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Travel_Wait_Hours_Nurse__c &gt; Billing_Code__r.Max__c,
OR(
(LEFT( Billing_Code__r.Name, 1) = &quot;T&quot;),
(LEFT(Billing_Code__r.Name, 1) = &quot;W&quot;)))</errorConditionFormula>
        <errorDisplayField>Travel_Wait_Hours_Nurse__c</errorDisplayField>
        <errorMessage>Travel/Wait units (Nurse) units exceed the Maximum allowable guideline. Please adjust.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MinAllowableLimitsProfNurse</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Professional_Hours_Nurse__c &lt; Billing_Code__r.Min__c, 
LEFT( Billing_Code__r.Name, 1) &lt;&gt; &quot;T&quot;, 
LEFT(Billing_Code__r.Name, 1) &lt;&gt; &quot;W&quot;)</errorConditionFormula>
        <errorDisplayField>Professional_Hours_Nurse__c</errorDisplayField>
        <errorMessage>Professional units do not meet the Minimum allowable time entry.  Please adjust.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MinAllowableLimitsTravelWait</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Travel_Wait_Hours_Nurse__c   &lt;  Billing_Code__r.Min__c,
OR(
(LEFT(Billing_Code__r.Name, 1) = &quot;T&quot;), 
(LEFT(Billing_Code__r.Name, 1) = &quot;W&quot;)))</errorConditionFormula>
        <errorDisplayField>Travel_Wait_Hours_Nurse__c</errorDisplayField>
        <errorMessage>Travel/Wait Units (Nurse) units do not meet the Minimum allowable guideline.  Please adjust.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Change_After_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
( Report__r.A_Notes_Approved__c =True),
OR(
 ISCHANGED(Professional_Hours__c ),
 ISCHANGED(Travel_Wait_Hours__c ),
ISCHANGED(Mileage__c )
)
)</errorConditionFormula>
        <errorMessage>Case Notes have been submitted and Approved.  The Case Notes can not be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Professional_Code_with_Wait_Hours</fullName>
        <active>true</active>
        <description>Throws an error if the Travel / Wait time is filled in</description>
        <errorConditionFormula>AND(
(LEFT( Billing_Code__r.Name, 1)  &lt;&gt;  &quot;W&quot;),
(LEFT( Billing_Code__r.Name, 1)  &lt;&gt;  &quot;T&quot;),
OR(
(Travel_Wait_Hours_Nurse__c &gt; 0.0),
(Travel_Wait_Hours_Nurse__c = 0.0),
(Mileage_Nurse__c = 0.0),
(Mileage_Nurse__c &gt; 0.0), 
(Driving_To_City__c &lt;&gt; null),
(Driving_From_City__c &lt;&gt; null)))</errorConditionFormula>
        <errorMessage>When using a Professional Activity Code, you must ONLY enter units in the Professional Units field.  Please remove any data in the Travel/Wait, Mileage, To (City) and/or From (City).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TravelCodeWithProfessionalHours</fullName>
        <active>true</active>
        <description>Throws an error if the Professional time is filled in</description>
        <errorConditionFormula>AND(
Professional_Hours_Nurse__c &gt;0.0,
OR(
(LEFT( Billing_Code__r.Name, 1) = &quot;T&quot;),
(LEFT(Billing_Code__r.Name, 1) = &quot;W&quot;)))</errorConditionFormula>
        <errorDisplayField>Billing_Code__c</errorDisplayField>
        <errorMessage>When using a Travel / Wait Activity Code, you must ONLY enter units in the Travel/Wait Units field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TravelRequireHoursMileageCity</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
(LEFT(Billing_Code__r.Name, 1) = &quot;T&quot;),
OR(
(Driving_To_City__c = null),
(Driving_From_City__c = null)))</errorConditionFormula>
        <errorMessage>Activity Code is Travel, please verify that Travel/Wait Units (Nurse), From (City) and/or To (City) have been populated before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Travel_Code_with_Professional_Hours</fullName>
        <active>true</active>
        <description>Throws an error if the Professional time is filled in</description>
        <errorConditionFormula>AND(
(LEFT( Billing_Code__r.Name, 1) = &quot;T&quot;),
AND(
( Professional_Hours_Nurse__c &gt;0.0)
))</errorConditionFormula>
        <errorMessage>When using a Travel / Wait Activity Code, you must ONLY enter units in the Travel/Wait Units field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Travel_Hours_Mileage_and_ToFrom_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
(LEFT(Billing_Code__r.Name, 1) = &quot;T&quot;),
OR(
(ISBLANK(Travel_Wait_Hours_Nurse__c)),
(ISBLANK(Driving_To_City__c)),
(ISBLANK(Driving_From_City__c))))</errorConditionFormula>
        <errorMessage>Activity Code is Travel, please verify that Travel/Wait Units (Nurse), From (City) and/or To (City) have been populated before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WaitCodeNoMileageCity</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
(LEFT(Billing_Code__r.Name, 1) = &quot;W&quot;),
OR(
(Mileage_Nurse__c  &gt; 0.0),
(Driving_To_City__c &lt;&gt; null),
(Driving_From_City__c &lt;&gt; null)))</errorConditionFormula>
        <errorMessage>Activity Code is Wait, please verify that Mileage (Nurse), From (City) and/or To (City) are NOT populated before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Wait_Code_with_Professional_Hours</fullName>
        <active>true</active>
        <description>Throws an error if the Professional time is filled in</description>
        <errorConditionFormula>AND(
(LEFT( Billing_Code__r.Name, 1) = &quot;W&quot;),
AND(
( Professional_Hours_Nurse__c &gt;0.0)
))</errorConditionFormula>
        <errorMessage>When using a Travel / Wait Activity Code, you must ONLY enter units in the Travel/Wait Units field.</errorMessage>
    </validationRules>
